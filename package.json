{
  "name": "@aureooms/js-integer-big-endian",
  "description": "Arbitrary precision arithmetic for integers in big endian order for JavaScript",
  "version": "10.1.0",
  "license": "AGPL-3.0",
  "author": "Aur√©lien Ooms <aurelien.ooms@gmail.com>",
  "homepage": "https://aureooms.github.io/js-integer-big-endian",
  "repository": {
    "type": "git",
    "url": "https://github.com/aureooms/js-integer-big-endian"
  },
  "bugs": {
    "url": "https://github.com/aureooms/js-integer-big-endian/issues"
  },
  "keywords": [
    "arbitrary",
    "arithmetic",
    "big",
    "endian",
    "integers",
    "javascript",
    "js",
    "numbers",
    "precision"
  ],
  "sideEffects": false,
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "babel --delete-dir-on-start --env-name production src -d lib",
    "cover": "c8 --all --src src --reporter=lcov npm test",
    "esdoc": "esdoc",
    "lint-config": "fixpack --dryRun",
    "lint-config-and-fix": "fixpack || fixpack",
    "prepare": "npm run build",
    "release": "np",
    "test": "ava"
  },
  "dependencies": {},
  "devDependencies": {
    "@aureooms/js-array": "4.0.0",
    "@aureooms/js-itertools": "5.0.1",
    "@aureooms/js-memory": "4.0.0",
    "@aureooms/js-operator": "1.0.2",
    "@aureooms/js-random": "2.0.0",
    "@babel/cli": "7.13.10",
    "@babel/core": "7.13.10",
    "@babel/preset-env": "7.13.10",
    "@babel/register": "7.13.8",
    "ava": "3.15.0",
    "babel-plugin-transform-remove-console": "6.9.4",
    "babel-plugin-unassert": "3.0.1",
    "babel-preset-power-assert": "3.0.0",
    "c8": "7.6.0",
    "coveralls": "3.1.0",
    "esdoc": "1.1.0",
    "esdoc-inject-script-plugin": "1.0.0",
    "esdoc-inject-style-plugin": "1.0.0",
    "esdoc-standard-plugin": "1.0.0",
    "fixpack": "4.0.0",
    "np": "7.4.0",
    "power-assert": "1.6.1",
    "regenerator-runtime": "0.13.7",
    "xo": "0.38.2",
    "xorshift.js": "1.0.5"
  },
  "ava": {
    "files": [
      "test/src/**/*"
    ],
    "require": [
      "regenerator-runtime/runtime",
      "@babel/register"
    ],
    "timeout": "3m",
    "concurrency": 8
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "env": {
      "test": {
        "presets": [
          "babel-preset-power-assert"
        ],
        "plugins": [
          [
            "transform-remove-console",
            {
              "exclude": [
                "log",
                "error",
                "warn"
              ]
            }
          ]
        ],
        "sourceMaps": "inline"
      },
      "development": {
        "presets": [
          "babel-preset-power-assert"
        ],
        "plugins": [
          [
            "transform-remove-console",
            {
              "exclude": [
                "log",
                "error",
                "warn"
              ]
            }
          ]
        ],
        "sourceMaps": "inline"
      },
      "production": {
        "plugins": [
          "babel-plugin-unassert",
          [
            "transform-remove-console",
            {
              "exclude": [
                "log",
                "error",
                "warn"
              ]
            }
          ]
        ],
        "sourceMaps": "inline"
      }
    }
  }
}
